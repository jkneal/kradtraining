    <xsd:attribute name="exclusiveMin">
      <xsd:annotation>
        <xsd:documentation source="org.kuali.rice.krad.datadictionary.AttributeDefinition" xml:lang="en"><![CDATA[documentation not available]]></xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="label">
      <xsd:annotation>
        <xsd:documentation source="org.kuali.rice.krad.datadictionary.AttributeDefinition" xml:lang="en"><![CDATA[documentation not available]]></xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="inclusiveMax">
      <xsd:annotation>
        <xsd:documentation source="org.kuali.rice.krad.datadictionary.AttributeDefinition" xml:lang="en"><![CDATA[documentation not available]]></xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="shortLabel">
      <xsd:annotation>
        <xsd:documentation source="org.kuali.rice.krad.datadictionary.AttributeDefinition" xml:lang="en"><![CDATA[documentation not available]]></xsd:documentation>
