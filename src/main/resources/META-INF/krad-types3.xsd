      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="componentCode">
      <xsd:annotation>
        <xsd:documentation source="org.kuali.rice.krad.uif.layout.GridLayoutManager" xml:lang="en"><![CDATA[documentation not available]]></xsd:documentation>
