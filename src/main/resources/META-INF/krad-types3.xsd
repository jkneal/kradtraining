        <xsd:documentation source="org.kuali.rice.krad.datadictionary.AttributeDefinition" xml:lang="en"><![CDATA[documentation not available]]></xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="forceUppercase">
      <xsd:annotation>
        <xsd:documentation source="org.kuali.rice.krad.datadictionary.AttributeDefinition" xml:lang="en"><![CDATA[documentation not available]]></xsd:documentation>
